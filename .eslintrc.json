{
  "extends": "airbnb",
  "plugins": [
    "react",
    "react-native",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "react/require-default-props": 0,
    "no-use-before-define": 0
  }
}
